    "keywords": ["cron", "schedule", "time"],
    "homepage": "http://github.com/predakanga/CronBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "predakanga",
            "email": "predatory.kangaroo@gmail.com"
        },
        {
            "name": "mysiar",
            "email": "psynowiec@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^4.0",
        "doctrine/doctrine-bundle": "^1.7"
    },
    "autoload": {
        "psr-4": { "ColourStream\\CronBundle\\": "src/" }
